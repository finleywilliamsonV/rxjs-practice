{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "no-console": "off",
        "no-alert": "off",
        "semi": [
            "error",
            "never"
        ],
        "quotes": [
            2,
            "single"
        ],
        "no-use-before-define": "off"
    }
}